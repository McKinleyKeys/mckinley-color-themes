{
	"name": "McKinley's Color Themes",
	"type": "light",
	"colors": {
		
		"editor.background": "#f8f6f3",
		"editor.foreground": "#222",
		"editor.lineHighlightBackground": "#e2e7f3",
		"editorCursor.background": "#000",
		"editor.selectionBackground": "#A4CDFF",
		
		"focusBorder": "#00000000",
		
		"editor.findMatchBackground": "#fffa5c",
		"editor.findMatchHighlightBackground": "#e5e7f0",
		"editor.selectionHighlightBackground": "#e5e7f0",
		"editorBracketMatch.background": "#00000000",
		"editorBracketMatch.border": "#00000000",
		
		"editorLineNumber.foreground": "#a0a09e",
		"editorLineNumber.activeForeground": "#222325",
		"editor.foldBackground": "#f00",
		"editorGutter.modifiedBackground": "#3B82F7",
		"editorGutter.addedBackground": "#3B82F7",
		"editorGutter.deletedBackground": "#3B82F7",
		
		"editorGroupHeader.tabsBackground": "#f1f0f7",
		"editorGroupHeader.tabsBorder": "#e4e3e2",
		"editorGroup.border": "#e4e3e2",
		"tab.activeForeground": "#027bfe",
		"tab.activeBackground": "#cfe6fd",
		"tab.inactiveForeground": "#222",
		"tab.inactiveBackground": "#00000000",
		"tab.border": "#ddd",
		
		"activityBar.foreground": "#4a445f",
		"activityBar.inactiveForeground": "#888",
		"activityBar.background": "#d6d0ef",
		"activityBarBadge.foreground": "#fff",
		"activityBarBadge.background": "#0070f5",
		"activityBar.activeBorder": "#0070f5",
		"activityBar.border": "#c1bbd4",
		
		"sideBar.foreground": "#000",
		"sideBar.background": "#dde1ea",
		"sideBar.border": "#d6d6dd",
		"sideBarTitle.foreground": "#000",
		"sideBarSectionHeader.foreground": "#000",
		"sideBarSectionHeader.background": "#bdb7d2",
		"tree.indentGuidesStroke": "#d8dee4",
		"list.activeSelectionForeground": "#222",
		"list.activeSelectionBackground": "#bcb7d2",
		"list.activeSelectionIconForeground": "#222",
		"list.inactiveSelectionBackground": "#bcb7d2",
		"list.hoverBackground": "#00000000"
	},
	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#888895"
			}
		},
		{
			"name": "Comments: Documentation",
			"scope": [
				"comment.documentation",
				"comment.block.documentation"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#7b36ff",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"keyword.operator",
				"storage.type.function.arrow"
			],
			"settings": {
				"foreground": "#000",
				"fontStyle": ""
			}
		},
		{
			"name": "Types",
			"scope": [
				"storage.type",
				"support.type"
			],
			"settings": {
				"foreground": "#6600FF"
			}
		},
		{
			"name": "Language Constants",
			"scope": [
				"constant.language",
				"support.constant",
				"variable.language",
				"constant.numeric"
			],
			"settings": {
				"foreground": "#6400DC"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable.other.readwrite",
				"support.variable"
			],
			"settings": {
				"foreground": "#00A400"
			}
		},
		{
			"name": "Variable Definitions",
			"scope": [
				"meta.definition.variable",
				"variable.other.readwrite.alias"
			],
			"settings": {
				"foreground": "#222"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"meta.function-call entity.name.function"
			],
			"settings": {
				"foreground": "#ff7a00"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"meta.definition.function"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Classes",
			"scope": [
				"entity.name.type",
				"entity.other.inherited-class",
				"support.class"
			],
			"settings": {
				"foreground": "#ff2f8a"
			}
		},
		{
			"name": "Exceptions",
			"scope": "entity.name.exception",
			"settings": {
				"foreground": "#660000"
			}
		},
		{
			"name": "Sections",
			"scope": "entity.name.section",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"constant.character",
				"string",
				"punctuation.definition.string.begin",
				"punctuation.definition.string.end",
				"punctuation.definition.string.template.begin",
				"punctuation.definition.string.template.end"
			],
			"settings": {
				"foreground": "#00b4a3"
			}
		},
		{
			"name": "Strings: Escape Sequences",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Strings: Regular Expressions",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#4B83CD"
			}
		},
		{
			"name": "Strings: Symbols",
			"scope": "constant.other.symbol",
			"settings": {
				"foreground": "#AB6526"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation",
			"settings": {
				"foreground": "#777777"
			}
		},
		
		
		
		
		
		{
			"name": "HTML: Doctype Declaration",
			"scope": [
				"meta.tag.sgml.doctype",
				"meta.tag.sgml.doctype string",
				"meta.tag.sgml.doctype entity.name.tag",
				"meta.tag.sgml punctuation.definition.tag.html"
			],
			"settings": {
				"foreground": "#AAAAAA"
			}
		},
		{
			"name": "HTML: Tags",
			"scope": [
				"meta.tag",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html"
			],
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "HTML: Tag Names",
			"scope": [
				"entity.name.tag"
			],
			"settings": {
				"foreground": "#7b36ff",
				"fontStyle": "bold"
			}
		},
		{
			"name": "HTML: Attribute Names",
			"scope": [
				"meta.tag entity.other.attribute-name",
				"entity.other.attribute-name.html"
			],
			"settings": {
				"foreground": "#792fe4"
			}
		},
		{
			"name": "HTML: Entities",
			"scope": [
				"constant.character.entity",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#AB6526"
			}
		},
		
		
		
		{
			"name": "CSS: Selectors",
			"scope": [
				"meta.selector",
				"meta.selector entity",
				"meta.selector entity punctuation",
				"entity.name.tag.css"
			],
			"settings": {
				"foreground": "#7A3E9D"
			}
		},
		{
			"name": "CSS: Property Names",
			"scope": [
				"meta.property-name",
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#AB6526"
			}
		},
		{
			"name": "CSS: Property Values",
			"scope": [
				"meta.property-value",
				"meta.property-value constant.other",
				"support.constant.property-value"
			],
			"settings": {
				"foreground": "#448C27"
			}
		},
		{
			"name": "CSS: Important Keyword",
			"scope": "keyword.other.important",
			"settings": {
				"fontStyle": "bold"
			}
		},
		
		
		
		
		
		
		
		{
			"name": "Markup: Changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup: Deletion",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup: Emphasis",
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markup: Error",
			"scope": "markup.error",
			"settings": {
				"foreground": "#660000"
			}
		},
		{
			"name": "Markup: Insertion",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup: Link",
			"scope": "meta.link",
			"settings": {
				"foreground": "#4B83CD"
			}
		},
		{
			"name": "Markup: Output",
			"scope": [
				"markup.output",
				"markup.raw"
			],
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Markup: Prompt",
			"scope": "markup.prompt",
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Markup: Heading",
			"scope": "markup.heading",
			"settings": {
				"foreground": "#AA3731"
			}
		},
		{
			"name": "Markup: Strong",
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Traceback",
			"scope": "markup.traceback",
			"settings": {
				"foreground": "#660000"
			}
		},
		{
			"name": "Markup: Underline",
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Markup Quote",
			"scope": "markup.quote",
			"settings": {
				"foreground": "#7A3E9D"
			}
		},
		{
			"name": "Markup Lists",
			"scope": "markup.list",
			"settings": {
				"foreground": "#4B83CD"
			}
		},
		{
			"name": "Markup Styling",
			"scope": [
				"markup.bold",
				"markup.italic"
			],
			"settings": {
				"foreground": "#448C27"
			}
		},
		{
			"name": "Markup Inline",
			"scope": "markup.inline.raw",
			"settings": {
				"fontStyle": "",
				"foreground": "#AB6526"
			}
		},
		{
			"name": "Extra: Diff Range",
			"scope": [
				"meta.diff.range",
				"meta.diff.index",
				"meta.separator"
			],
			"settings": {
				"foreground": "#434343"
			}
		},
		{
			"name": "Extra: Diff From",
			"scope": "meta.diff.header.from-file",
			"settings": {
				"foreground": "#434343"
			}
		},
		{
			"name": "Extra: Diff To",
			"scope": "meta.diff.header.to-file",
			"settings": {
				"foreground": "#434343"
			}
		}
	]
}

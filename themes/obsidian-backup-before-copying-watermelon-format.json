	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#888895"
			}
		},
		{
			"name": "Comments: Documentation",
			"scope": [
				"comment.documentation",
				"comment.block.documentation"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#7b36ff",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"keyword.operator",
				"storage.type.function.arrow"
			],
			"settings": {
				"foreground": "#000",
				"fontStyle": ""
			}
		},
		{
			"name": "Types",
			"scope": [
				"storage.type",
				"support.type"
			],
			"settings": {
				"foreground": "#6600FF"
			}
		},
		{
			"name": "Language Constants",
			"scope": [
				"constant.language",
				"support.constant",
				"variable.language",
				"constant.numeric"
			],
			"settings": {
				"foreground": "#6400DC"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable.other.readwrite",
				"support.variable"
			],
			"settings": {
				"foreground": "#00A400"
			}
		},
		{
			"name": "Variable Definitions",
			"scope": [
				"meta.definition.variable",
				"variable.other.readwrite.alias"
			],
			"settings": {
				"foreground": "#222"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"meta.function-call entity.name.function"
			],
			"settings": {
				"foreground": "#ff7a00"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"meta.definition.function"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Classes",
			"scope": [
				"entity.name.type",
				"entity.other.inherited-class",
				"support.class"
			],
			"settings": {
				"foreground": "#ff2f8a"
			}
		},
		{
			"name": "Exceptions",
			"scope": "entity.name.exception",
			"settings": {
				"foreground": "#660000"
			}
		},
		{
			"name": "Sections",
			"scope": "entity.name.section",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"constant.character",
				"string",
				"punctuation.definition.string.begin",
				"punctuation.definition.string.end",
				"punctuation.definition.string.template.begin",
				"punctuation.definition.string.template.end"
			],
			"settings": {
				"foreground": "#00b4a3"
			}
		},
		{
			"name": "Strings: Escape Sequences",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Strings: Regular Expressions",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#4B83CD"
			}
		},
		{
			"name": "Strings: Symbols",
			"scope": "constant.other.symbol",
			"settings": {
				"foreground": "#AB6526"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation",
			"settings": {
				"foreground": "#777777"
			}
		},
		
		
		
		
		
